@model ResourceListViewModel

<div class="card">
    <div class="card-title">@Html.DisplayFor(model => model.ResourceType) Resources</div>
    <div class="card-body">
        <div class="table">
            <table class="table-striped">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Type</th>
                    <th>Location</th>
                    <th>Migrated</th>
                    <th>Notes</th>
                    <th></th>
                </tr>
                </thead>
                <tbody>
                @Html.DisplayFor(model => model.Resources)
                </tbody>
            </table>            
        </div>
    </div>
</div>