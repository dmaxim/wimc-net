@model NewResourceViewModel

<h3>Azure Resources</h3>
<div class="row">
    <form asp-action="UploadJson" enctype="multipart/form-data">
        <div class="card">
            <div class="card-header">Upload Doc</div>
            <div class="card-body">
                @Html.EditorForModel()
            </div>
            <div class="form-group text-right">
                <input type="submit" value="Upload" class="btn btn-primary"/>
            </div>
        </div>

    </form>
</div>