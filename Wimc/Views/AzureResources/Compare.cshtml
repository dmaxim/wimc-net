@model ResourceComparisonViewModel

<div class="card">
    <div class="card-title">Actions</div>
    <div class="card-body">
         <a asp-action="AddNewResources" asp-controller="AzureResources" asp-route-id="@Model.ContainerId" ><span class="material-icons" title="Add New">notes</span></a>
    </div>
</div>
<div class="card">
    <div class="card-title">
        New Resources - @Html.DisplayFor(model => model.ContainerName)
    </div>
    <div class="card-body table">
            <table class="table-striped">
                <thead>
                <tr>
                    <th>Name</th>
                    <th>Type</th>
                    <th>Location</th>
                    <th>Id</th>
                    <th>Actions</th>
                </tr>
                </thead>
                <tbody>
                @Html.DisplayFor(model => model.New)
                </tbody>
            </table>
    </div>
    </div>


<div class="card">
    <div class="card-title">
        Deleted Resources
    </div>
    <div class="card-body table">
                    <table class="table-striped">
                        <thead>
                        <tr>
                            <th>Name</th>
                            <th>Type</th>
                            <th>Location</th>
                            <th>Migrated</th>
                            <th>Notes</th>
                            <th>Actions</th>
                        </tr>
                        </thead>
                        <tbody>
                        @Html.DisplayFor(model => model.Deleted)
                        </tbody>
                    </table>
    </div>
</div>